{
  "coc.preferences.formatOnSaveFiletypes": ["css", "markdown",
    "javascript", "graphql", "html", "yaml",  "json", "python",
    "javascriptreact", "typescript", "typescriptreact"],
  "explorer.width": 30,
  "explorer.buffer.root.template": "[icon & 1] OPEN EDITORS",
  "explorer.file.root.template": "[icon & 1] ROOT ([root])",
  "explorer.file.child.template": "[git | 2] [selection | clip | 1] [indent][icon | 1] [diagnosticError & 1][filename omitCenter 1][modified][readonly] [linkIcon & 1][link growRight 1 omitCenter 5]",
  "explorer.file.showHiddenFiles": true,
  "explorer.file.autoReveal": false,
  "explorer.icon.enableVimDevicons": true,
  "explorer.icon.enableNerdfont": true,
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings": {
    "s": "open:vsplit",
    "mm": "rename",
    "mc": "copyFile",
    "C": "copyFile",
    "md": "delete",
    "D": "delete",
    "ma": "addFile",
    "mA": "addDirectory"
  },
  "coc.source.emoji.filetypes": ["markdown", "typescript", "cpp", "javascript", "javascriptreact", "typescriptreact"],
  "texlab.path": "~/ProgramFiles/TextLab/texlab"
}
